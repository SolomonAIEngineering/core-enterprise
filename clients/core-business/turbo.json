{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env",
    "$BITLY_CLIENT_SECRET",
    "$PROFITWELL_API_KEY",
    "$PROJECT_ID_VERCEL",
    "$TEAM_ID_VERCEL",
    "$AUTH_BEARER_TOKEN",
    "$NODE_ENV",
    "$VERCEL",
    "$PLAIN_WEBHOOK_SECRET",
    "$DUB_WEBHOOK_SECRET",
    "$STRIPE_CONNECT_WEBHOOK_SECRET",
    "$STRIPE_APP_WEBHOOK_SECRET",
    "$STRIPE_WEBHOOK_SECRET",
    "$UNSPLASH_ACCESS_KEY",
    "$NEXTAUTH_URL",
    "$NEXTAUTH_SECRET",
    "$STORAGE_SECRET_ACCESS_KEY",
    "$PANGEA_API_KEY",
    "$GOOGLE_CLIENT_ID",
    "$GOOGLE_CLIENT_SECRET",
    "$GITHUB_CLIENT_ID",
    "$GITHUB_CLIENT_SECRET",
    "$QSTASH_TOKEN",
    "$QSTASH_CURRENT_SIGNING_KEY",
    "$QSTASH_NEXT_SIGNING_KEY",
    "$CRON_SECRET",
    "$DYNADOT_API_KEY",
    "$DYNADOT_COUPON",
    "$EDGE_CONFIG",
    "$EDGE_CONFIG_ID",
    "$DATABASE_URL",
    "$PLANETSCALE_DATABASE_URL",
    "$PLAIN_API_KEY",
    "$RESEND_API_KEY",
    "$RESEND_AUDIENCE_ID",
    "$STORAGE_ACCESS_KEY_ID",
    "$STORAGE_ENDPOINT",
    "$STRIPE_SECRET_KEY",
    "$TINYBIRD_API_KEY",
    "$TINYBIRD_API_URL",
    "$TINYBIRD_DEMO_API_KEY",
    "$UPSTASH_REDIS_REST_URL",
    "$UPSTASH_REDIS_REST_TOKEN",
    "$VERCEL_URL"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["!.next/cache/**", ".next/**", "dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "test": {}
  }
}
